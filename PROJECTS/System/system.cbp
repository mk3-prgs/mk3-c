				<Linker>
					<Add option="-mcpu=cortex-m3" />
					<Add option="-mtune=cortex-m3" />
					<Add option="-mthumb" />
					<Add option="-specs=nano.specs" />
					<Add option="-specs=nosys.specs" />
					<Add option="-nostartfiles" />
					<Add option="-Wl,-Map=bin/system.map,--cref" />
					<Add option="-L./ld" />
					<Add option="-Wl,-Tstm32f103rc.ld" />
					<Add option="-Wl,-R,../bin/system.sym" />
					<Add option="-Wl,--gc-sections" />
				</Linker>
				<ExtraCommands>
					<Add before="rm -f $(TARGET_OUTPUT_FILE).sym" />
					<Add before="# rm -f $(TARGET_OUTPUT_FILE).bin" />
					<Add before="rm -f $(TARGET_OUTPUT_FILE).hex" />
					<Add after="# arm-none-eabi-objcopy -O ihex -R .text -R .rodata -R .data -R .bss -R .bss0 $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE)_b0.hex" />
					<Add after="# arm-none-eabi-objcopy -S -O binary $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).bin" />
					<Add after="# arm-none-eabi-nm -n $(TARGET_OUTPUT_FILE) &gt; $(TARGET_OUTPUT_FILE).sss" />
					<Add after="arm-none-eabi-size $(TARGET_OUTPUT_FILE)" />
					<Add after="arm-none-eabi-objcopy -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).hex" />
					<Add after="arm-none-eabi-objcopy -x --extract-symbol $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).sym" />
				</ExtraCommands>
			</Target>
		</Build>
